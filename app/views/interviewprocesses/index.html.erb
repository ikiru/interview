<p id="notice"><%= notice %></p>

<h1>Listing Interviewprocesses</h1>

<table>
  <thead>
    <tr>
      <th>Interview number</th>
      <th>Position</th>
      <th>Interview type</th>
      <th>Question</th>
      <th>Project</th>
      <th>Comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @interviewprocesses.each do |interviewprocess| %>
      <tr>
        <td><%= interviewprocess.interview_number %></td>
        <td><%= interviewprocess.position_id %></td>
        <td><%= interviewprocess.interview_type_id %></td>
        <td><%= interviewprocess.question_id %></td>
        <td><%= interviewprocess.project_id %></td>
        <td><%= interviewprocess.comments %></td>
        <td><%= link_to 'Show', interviewprocess %></td>
        <td><%= link_to 'Edit', edit_interviewprocess_path(interviewprocess) %></td>
        <td><%= link_to 'Destroy', interviewprocess, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Interviewprocess', new_interviewprocess_path %>
