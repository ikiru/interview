<%= simple_form_for(@interviewprocess) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :interview_number %>
    <%= f.input :position_id %>
    <%= f.input :interview_type_id %>
    <%= f.input :question_id %>
    <%= f.input :project_id %>
    <%= f.input :comments %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
